l_36 = f.readline()
l_37 = f.readline()
l_38 = f.readline()
l_39 = f.readline()
l_40 = f.readline()
l_41 = f.readline()

# assigning input to variables
l_LE = float(l_4[45:])
LE = l_LE
l_ZR = float(l_5[45:])
ZO = l_ZR
l_A2 = float(l_9[45:])
l_A3 = float(l_10[45:])
l_A4 = float(l_11[45:])	
l_A5 = float(l_12[45:])	
l_IO2 = float(l_13[45:])	
l_IO3 = float(l_14[45:])	
l_IO4 = float(l_15[45:])	
l_IO5 = float(l_16[45:])
l_ZR = float(l_17[45:])
l_ZI = float(l_18[45:])
l_SI = float(l_22[45:])
l_RI = float(l_23[45:])	
l_II = float(l_24[45:])	
l_ZV = float(l_25[45:])	
l_ZP = float(l_26[45:])	
l_EA2 = float(l_30[45:])
l_EW = float(l_31[45:])	
l_CO = float(l_32[45:])
l_EI = float(l_33[45:])
l_AO = float(l_37[45:])	
l_SI2 = float(l_38[45:])	
l_RI2 = float(l_39[45:])	
l_II2 = float(l_40[45:])	
l_WO2 = float(l_41[45:])	


print(
'''
************** Main Menu Options **************
 0 Show the content of "input.txt".
 1 NPA graph with varied z-number 
